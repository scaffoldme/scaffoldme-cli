  "dependencies": {
    "debug": "^4.0.0",
    "scaffoldme": "1.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.1",
    "tslint": "^5.9.1",
    "typescript": "^3.4.5"
  }
